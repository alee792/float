<section class="section floating-fill">
    <div class="container">
        {{#each feedback}}
        <div class="card">
            <div class="card-content">
                <article class="media">
                    <figure class="media-left">
                        <p class="image is-64x64">
                            <img src="{{ this._post.post_plan.avatar_image }}">
                        </p>
                    </figure>
                    <div class="media-content">
                        <div class="content">
                            <p>
                                <strong>{{ this._post.post_plan.post_account }} by {{this._post.created_by}}</strong>
                                <br> {{ this._post.post_candidate.content }}
                                <br>
                                <small>
                                    <a>Like</a> 路
                                    <a>Reply</a> 路 {{ this._post.created_at }}</small>
                            </p>
                        </div>

                        <article class="media">
                            <figure class="media-left">
                                <p class="image is-48x48">
                                    <img src="https://bulma.io/images/placeholders/96x96.png">
                                </p>
                            </figure>
                            <div class="media-content">
                                <div class="content">
                                    <p>
                                        <strong>Kayli Eunice </strong>
                                        <br>
                                        <span class="edit-content">{{this.notes}}</span>
                                        <br>
                                        <small>
                                            <a class="edit" onclick="editFeedbackClick(this, '{{this._id}}')">Edit</a> 路
                                            <a onclick="deleteFeedbackClick('{{this._id}}')">Delete</a> 路 {{this.created_at}}
                                        </small>
                                    </p>
                                </div>
                            </div>
                        </article>
                    </div>
                </article>
            </div>
        </div>
        <br> {{/each}}
    </div>
</section>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="/js/feedback.js"></script>